@using do_an_cuoi_ky.Models;
@model IEnumerable<CHUDE>

<div class="title">CHỦ ĐỀ MÔ HÌNH</div>
<ul class="list-group">
    @foreach (var chude in Model)
    {
        <a href="@Url.Action("SPTheoChuDe", "Shop", new { id=chude.MaCD })"
           class="list-group list-group-horizontal">
            @chude.TenChuDe
        </a>
    }
</ul>
